<?xml version="1.0"?>
<!--<!DOCTYPE metadata SYSTEM "http://www.esri.com/metadata/esriprof80.dtd">-->
<metadata xml:lang="en">
  <tool name="select" displayname="Select" toolboxalias="" softwarerestriction="none">
    <summary>
      <para>Creates a database file with the selected values.</para>
    </summary>
    <toolIllust src="" alt="No internet connection. Run localimages.py to view images." vsrc="http://somanalyst.googlecode.com/svn/trunk/doc/sphinx/_images/toolbox/selectV.png"/>
    <parameters>
      <param name="input_database_file" displayname="input database file" type="Required" direction="Input" expression="&lt;input_database_file&gt;"/>
      <param name="selection_type" displayname="selection type" type="Required" direction="Input" expression="&lt;columns | rows&gt;">
        <commandReference/>
        <dialogReference>
          <para>The way in which data will be selected.</para>
        </dialogReference>
      </param>
      <param name="output_database_file" displayname="output database file" type="Required" direction="Output" expression="&lt;output_database_file&gt;"/>
      <param name="columns" displayname="columns" type="Optional" direction="Input" expression="{columns;columns...}">
        <commandReference/>
        <dialogReference>
          <para>The columns to include in the data selection.</para>
        </dialogReference>
      </param>
      <param name="start_index" displayname="start index" type="Optional" direction="Input" expression="{start_index}">
        <commandReference/>
        <dialogReference>
          <para>The index from which to begin the selection.</para>
        </dialogReference>
      </param>
      <param name="index_step" displayname="index step" type="Optional" direction="Input" expression="{index_step}">
        <commandReference/>
        <dialogReference>
          <para>The increment from the start index for the next selection.</para>
        </dialogReference>
      </param>
      <param name="stop_index" displayname="stop index" type="Optional" direction="Input" expression="{stop_index}">
        <commandReference/>
        <dialogReference>
          <para>The index at which to stop selection. Negative indicies are equivalent of indexing from the end.</para>
        </dialogReference>
      </param>
      <param name="detect_data_types" displayname="detect data types" type="Optional" direction="Input" expression="{detect_data_types}">
        <commandReference/>
        <dialogReference>
          <para>An optional mode that automatically changes the data types in a column based on the values it contains. This mode is usueful if the data will be processed further using  mathmatical operations.</para>
        </dialogReference>
      </param>
    </parameters>
  </tool>
  <dataIdInfo>
    <idCitation>
      <resTitle>select</resTitle>
      <citRespParty>
        <rpIndName>Martin Lacayo</rpIndName>
        <rpOrgName>Department of Geography, San Diego State University</rpOrgName>
        <rpPosName>M.S. Candidate</rpPosName>
        <role>
          <RoleCd>010</RoleCd>
        </role>
        <rpCntInfo>
          <cntAddress>
            <delPoint/>
            <city/>
            <adminArea/>
            <postCode/>
            <country/>
            <eMailAdd>somanalyst@gmail.com</eMailAdd>
          </cntAddress>
          <cntPhone>
            <voiceNum/>
            <faxNum/>
          </cntPhone>
        </rpCntInfo>
      </citRespParty>
    </idCitation>
    <idAbs></idAbs>
  </dataIdInfo>
</metadata>
