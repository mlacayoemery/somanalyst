<metadata xml:lang="en">
  <tool name="visual" displayname="3. Project Data onto SOM" toolboxalias="">
    <summary>
      <para>Creates a best matching unit file by projecting data onto a SOM.</para>
    </summary>
    <toolIllust src="" alt="No internet connection. Run localimages.py to view images." vsrc="http://somanalyst.googlecode.com/svn/trunk/doc/sphinx/_images/toolbox/visual.png"/>
    <parameters>
      <param name="SOM" displayname="SOM" type="Required" direction="Input" expression="&lt;SOM&gt;">
        <commandReference/>
        <dialogReference>
          <para>The SOM on which to project data.</para>
        </dialogReference>
      </param>
      <param name="data_to_project" displayname="data to project" type="Required" direction="Input" expression="&lt;data_to_project&gt;">
        <commandReference/>
        <dialogReference>
          <para>The data to project onto the SOM.</para>
        </dialogReference>
      </param>
      <param name="projected_data" displayname="projected data" type="Required" direction="Output" expression="&lt;projected_data&gt;">
        <commandReference/>
        <dialogReference>
          <para>The ouput file to contain the projected data.</para>
        </dialogReference>
      </param>
      <param name="distance_metric" displayname="distance metric" type="Optional" direction="Input" expression="{Euclidean | Cosine}">
        <commandReference/>
        <dialogReference>
          <para>The distance metric to use in projecting the data.</para>
        </dialogReference>
      </param>
      <param name="skip_masked_data_vectors" displayname="skip masked data vectors" type="Optional" direction="Input" expression="{skip_masked_data_vectors}">
        <commandReference/>
        <dialogReference>
          <para>Skip vectors that have been masked. Masking is done by begining vectors with a "x", but this is not reccomended since SOM Analyst in several instances does matching based on line number.</para>
        </dialogReference>
      </param>
      <param name="read_buffer" displayname="read buffer" type="Optional" direction="Input" expression="{read_buffer}">
        <commandReference/>
        <dialogReference>
          <para>The number of lines to read at a time.</para>
        </dialogReference>
      </param>
    </parameters>
    <toolIllust src="" alt=""/>
    <usage/>
    <scriptExample/>
  </tool>
