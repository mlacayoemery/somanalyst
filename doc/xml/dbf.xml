<metadata xml:lang="en">
  <tool name="dbf" displayname="1. Data File to Database File" toolboxalias="">
    <summary>
      <para>Converts various data formats to the database file format required for all other data processing tools. Currently comma separated value (CSV) and tab separated value (TSV) formats are supported.</para>
    </summary>
    <toolIllust src="" alt="No internet connection. Run localimages.py to view images." vsrc="http://somanalyst.googlecode.com/svn/trunk/doc/sphinx/_images/toolbox/dbfV.png"/>
    <arcToolboxHelpPath>C:\Program Files\ArcGIS\help\gp</arcToolboxHelpPath>
    <parameters>
      <param name="input_data_file" displayname="input data file" type="Required" direction="Input" expression="&lt;input_data_file&gt;">
        <commandReference/>
        <dialogReference>
          <para>The input data file to read.</para>
        </dialogReference>
      </param>
      <param name="input_file_format" displayname="input file format" type="Required" direction="Input" expression="&lt;Comma Separated Values (CSV) | Tab Separated Values(TSV)&gt;">
        <commandReference/>
        <dialogReference>
          <para>The input file format.</para>
          <illust src="E:\somanalyst\doc\sphinx\_images\toolbox\csv.png" alt=""/>
          <illust src="E:\somanalyst\doc\sphinx\_images\toolbox\tsv.png" alt=""/>
        </dialogReference>
      </param>
      <param name="output_database_file" displayname="output database file" type="Required" direction="Output" expression="&lt;output_database_file&gt;">
        <commandReference/>
        <dialogReference>
          <illust src="E:\somanalyst\doc\sphinx\_images\toolbox\dbf.png" alt=""/>
        </dialogReference>
      </param>
      <param name="detect_data_types" displayname="detect data types" type="Optional" direction="Input" expression="{detect_data_types}">
        <commandReference/>
        <dialogReference>
          <para>An optional mode that automatically detects the data type in a column.</para>
        </dialogReference>
      </param>
    </parameters>
    <commandExample>uiToXbase.py census.csv "Comma Separated Values (CSV)" census.dbf #</commandExample>
    <usage/>
    <scriptExample>uiToXbase.toXbase("census.csv","Comma Separated Values (CSV)","census.dbf",False)</scriptExample>
  </tool>
